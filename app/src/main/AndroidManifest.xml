<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="com.example.yangsan.READ_DATABASE"/>
    <uses-permission android:name="com.example.yangsan.WRITE_DATABASE"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_runyangsan_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_runyangsan_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RunYangsan"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RunYangsan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <queries>
        <package android:name="com.example.yangsan"/>
    </queries>

</manifest>